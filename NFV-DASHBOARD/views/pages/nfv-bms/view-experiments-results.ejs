<header class="page-header">
  <h2>Experiments Results</h2>

  <div class="right-wrapper text-right" style="margin-right: 20px">
    <ol class="breadcrumbs">
      <li>
        <a href="/nfv-bms/">
          <i class="fas fa-home"></i>
        </a>
      </li>
      <li><span>Experiments</span></li>
      <li><span>View Experiments Results</span></li>
    </ol>

  </div>
</header>


<div class="row">
  <div class="col-lg-12">
    <section class="card">

      <header class="card-header">
        <div class="card-actions">
          <a href="#" class="card-action card-action-toggle" data-card-toggle></a>
          <a href="#" class="card-action card-action-dismiss" data-card-dismiss></a>
        </div>

        <h2 class="card-title">View Experiments Results</h2>
      </header>

      <div class="card-body">
        <form class="form-horizontal form-bordered" action="/nfv-bms/view-experiments-results">


          <div class="row form-group">
            <label class="col-lg-3 control-label text-lg-right pt-2 required">Experiment ID</label>
            <div class="col-lg-6">
              <div class="input-group">
                <input type="text" placeholder="Enter experiment ID here..." class="form-control"
                       value="<%= req.query.experiment_id %>" name="experiment_id" required></div>
            </div>
          </div>

          <div class="form-group row">
            <div class="col-lg-12 text-right">
              <button type="submit" class="btn btn-success">View Experiments Results</button>
            </div>
            <div class="col-lg-12 text-right">
              <button type="submit" class="btn btn-danger">Start Experiment</button>
            </div>
          </div>
        </form>
      </div>
    </section>
  </div>
</div>

<% if(req.query.experiment_id && req.query.experiment_id !== ""){ %>
