</head>

<body>

<section class="body">

  <!-- start: header -->
  <header class="header header-nav-menu">
    <div class="logo-container">
      <a href="/" class="logo">
        <img src="/images/logo-white-text-right.png" height="35" alt="Porto Admin"/>
      </a>
      <button class="btn header-btn-collapse-nav d-lg-none" data-toggle="collapse" data-target=".header-nav">
        <i class="fas fa-bars"></i>
      </button>

      <!-- start: header nav menu -->
      <div class="header-nav collapse">
        <div class="header-nav-main header-nav-main-effect-1 header-nav-main-sub-effect-1">
          <nav>
            <ul class="nav nav-pills" id="mainNav">
              <li class="<%=(appName === "NFV-MON"?"active":"")%>">
                <a class="nav-link" href="/">
                  NFV-MON
                </a>
              </li>
              <li class="<%=(appName === "NFV-VMS"?"active":"")%>">
                <a class="nav-link" href="">
                  NFV-VMS
                </a>
              </li>
              <li class="<%=(appName === "NFV-BMS"?"active":"")%>">
