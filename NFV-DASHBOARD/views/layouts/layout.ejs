          nfv_vms_port = "<%= sails.config.custom.nfv_vms_port %>",
          nfv_bms_address = "<%= sails.config.custom.nfv_bms_address %>",
          nfv_bms_port = "<%= sails.config.custom.nfv_bms_port %>",
          nfv_lab_address = "<%= sails.config.custom.nfv_lab_address %>",
